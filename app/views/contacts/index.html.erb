<h1>Listing contacts</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Agent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.email %></td>
        <td><%= contact.agent_id %></td>
        <td><%= link_to 'Show', agent_contacts_path %></td>

      </tr>
    <% end %>
  </tbody>
</table>
