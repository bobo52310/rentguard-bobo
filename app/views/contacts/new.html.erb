<h1>New contact</h1>

<%= form_for(@contact, :url => agent_contacts_path(@agent,@contact)) do |e| %>

  <div class="field">
    <%= e.label :email %><br>
    <%= e.email_field :email %>
  </div>
  <div class="actions">
    <%= e.submit %>
  </div>
<% end %>

<%= link_to 'Back', agent_path(@agent) , :class => "btn btn-default"%>
